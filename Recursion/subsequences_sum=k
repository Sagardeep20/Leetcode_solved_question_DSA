#include<bits/stdc++.h>
using namespace std;

int ptintSubsequences_sumK(int idx , vector<int> &ans , int sum , int n , int arr[] , int k){
    if(idx >= n){
        if(sum == k){
            cout<<"printing the subsequences :- "<<endl;
            for(auto it:ans){
            cout<<it;
            }
            cout<<endl;
        }
        return 0;
    }

    ans.push_back(arr[idx]);
    sum += arr[idx];
    ptintSubsequences_sumK(idx+1 , ans , sum , n , arr , k);

    ans.pop_back();
    sum -= arr[idx];
    ptintSubsequences_sumK(idx+1 , ans , sum , n ,arr , k);

}
int main(){
    int n;
    cout<<"size of array:- ";
    cin>>n;

    int k = 2;

    int arr[n];
    cout<<"array elements:- "<<endl;
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    vector<int> ans;
    ptintSubsequences_sumK(0 , ans, 0 , n , arr , k);


}
