#include<bits/stdc++.h>
using namespace std;
int printSubsequences(int arr[] ,int idx, int n, vector<int> &ans){
    if(idx >= n){
        for(auto it:ans){
            cout<<it;
        }
        cout<<endl;

        return 0;
    }

    ans.push_back(arr[idx]);
    printSubsequences(arr , idx+1 , n , ans);
    ans.pop_back();
    printSubsequences(arr , idx+1 , n , ans);


}
int main(){
    
    int n;
    cout<<"size of array:- ";
    cin>>n;
    int arr[n];
    cout<<"array elements :- ";
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }

    cout<<"subsequences are :- ";
    vector<int> ans;
    printSubsequences(arr,0,n , ans);
}
