void preOrder(vector<int> &ans , TreeNode* root){
        if(root == NULL){
            return ;
        }
        ans.push_back(root->val);
        preOrder(ans , root->left);
        preOrder(ans , root->right);
    }
    vector<int> preorderTraversal(TreeNode* root) {
        vector<int> ans;
        preOrder(ans , root);
        return ans;
    }   TC-o(n)
        sc-o(n) worst case & o(logn) best/avg case
LEETCODE -144
