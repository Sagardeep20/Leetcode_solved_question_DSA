class Solution {
public:
    vector<int> findAnagrams(string s, string p) {
        vector<int>ans;
        int k = p.size();
        unordered_map<char,int>mpp;
        for(int i=0;i<p.size();i++){
            mpp[p[i]]++;
        }
        int cnt  = mpp.size();

        int i=0,j=0;
        while(j<s.size()){
            //condition
            if(mpp.find(s[j]) != mpp.end()){
                mpp[s[j]]--;
                if(mpp[s[j]] == 0){
                    cnt--;
                }
            }

            if(j-i+1 < k)j++;
            else if(j-i+1 == k){
                if(cnt == 0){
                    ans.push_back(i);
                }
                if(mpp.find(s[i]) != mpp.end()){
                    if(mpp[s[i]] == 0)cnt++;
                    mpp[s[i]]++;
                }
                i++;
                j++;

            }
        }
        return ans;
    }
};
