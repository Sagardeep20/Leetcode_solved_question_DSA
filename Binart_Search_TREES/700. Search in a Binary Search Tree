 TreeNode* searchBST(TreeNode* root, int val) {
        /*
        while(root!= NULL && root->val != val){
            root = (val < (root->val) ) ? (root->left) : (root->right);

        }
        return root;*/

        TreeNode* ans;
        while(root != NULL){
            if(root->val == val){
                ans = root;
                break;
            }

            if(val < root->val){
                root = root->left;
            }
            else{
                root = root->right;
            }
        }
        return ans;

    }
