
class Solution {
public:
    TreeNode* recCall(int st , int end , vector<int>& nums){
        if(st > end)return NULL;
        int mid = st + (end-st)/2;
        int val = nums[mid];
        TreeNode* newRoot = new TreeNode(val);
        newRoot->left = recCall(st , mid-1 , nums);
        newRoot->right = recCall(mid+1 , end , nums);
        return newRoot;

    } 
    TreeNode* sortedArrayToBST(vector<int>& nums) {
        return recCall(0,nums.size()-1,nums);
    }
};
