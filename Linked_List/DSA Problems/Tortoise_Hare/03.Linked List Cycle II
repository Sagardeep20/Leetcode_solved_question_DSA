/**
 * Definition for singly-linked list.
 * struct ListNode {
 *     int val;
 *     ListNode *next;
 *     ListNode(int x) : val(x), next(NULL) {}
 * };
 */
class Solution {
public:
    ListNode *detectCycle(ListNode *head) {
        /*
        unordered_map<ListNode*, int>mpp;
        ListNode* temp = head;
        if(temp == NULL || temp->next == NULL){
            return NULL;
        }
        int pos = 0;
        while(temp != NULL){
            if(mpp.find(temp) != mpp.end()){
                return temp;
            }
            mpp[temp] = pos;
            pos++;
            temp = temp->next;
        }
        return nullptr;

        Tc - o(N) DSC-o(N)
        */
        TC-o(N) SC-o(1)
        ListNode* slow = head;
        ListNode* fast = head;
        
        while(fast != NULL && fast->next != NULL){
            slow = slow->next;
            fast = fast->next->next;
            if(fast == slow)break;
        }

        if(fast == NULL || fast->next == NULL)return NULL;

        fast = head;
        while(fast != slow){
            fast = fast->next;
            slow = slow->next;
        }
        return slow;
    }
};
