class Solution {
public:
    string sortSentence(string s) {
        string temp;
        int cnt = 0;
        vector<string > ans(10);
        for(int i=0;i<s.size();i++){
            if(s[i] == ' '){
                int pos = temp[temp.size() - 1] - '0';
                temp.pop_back();
                ans[pos] = temp;
                temp.clear();
                cnt ++;
                
            }
            else{
                temp += s[i];
            }
        }
        int pos = temp[temp.size() - 1] - '0';
        temp.pop_back();
        ans[pos] = temp;
        temp.clear();
        cnt ++;

        string result = "";
        for(int i=1;i<=cnt;i++){
            result += ans[i];
            result += ' ';
        }
        result.pop_back();
        return result;
    }
};
