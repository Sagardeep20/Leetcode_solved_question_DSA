class Solution {
public:
    int subarraysDivByK(vector<int>& nums, int k) {
        unordered_map<int,int>reminderMpp;
        int reminder = 0;
        int cumuSum = 0,cnt = 0;
        reminderMpp[0] = 1;
        for(int i=0;i<nums.size();i++){
            cumuSum += nums[i];
            reminder = (cumuSum % k) < 0 ? (cumuSum%k)+k : (cumuSum%k);
            if(reminderMpp.find(reminder) != reminderMpp.end()){
                cnt = cnt + reminderMpp[reminder];
            }
            reminderMpp[reminder]++;
        }
        return cnt;
    }
};

lect - codestorywithmik
if(rem < 0) rem = rem+k
else rem= rem  by (cumulativesum % k)
