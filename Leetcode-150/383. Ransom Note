bool canConstruct(string ransomNote, string magazine) {
        unordered_map<char,int> mpp1,mpp2;
        for(int i=0;i<ransomNote.size();i++){
            mpp1[ransomNote[i]]++;
        }
        for(int i=0;i<magazine.size();i++){
            mpp2[magazine[i]]++;
        }
        if(mpp2.size() < mpp1.size())return false;
        for(auto &it : mpp1){
            char c = it.first;
            int freeq = it.second;

            if(mpp2[c] < freeq){
                return false;
            }
            else{
                continue;
            }
        }
        return true;
    }
