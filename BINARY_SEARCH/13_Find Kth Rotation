https://www.geeksforgeeks.org/problems/rotation4723/1

class Solution {
  public:
    int findKRotation(vector<int> &arr) {
        // Code Here
        int s= 0;
        int h = arr.size()-1;
        while(s<h){
            int mid = s +(h-s)/2;
            
            if(arr[mid] > arr[h]){
                s = mid+1;
            }else{
                h=mid;
            }
        }
        return s;
    }
};
