class Solution {
public:
    int findPeakElement(vector<int>& nums) {
        int s=0,h=nums.size() - 1;
        while(s < h){
            int mid = s+(h-s)/2;
            if(nums[mid] < nums[mid+1]){
                s = mid+1;
            }else{
                h = mid;
            }
        }
        return s;
    }
};
