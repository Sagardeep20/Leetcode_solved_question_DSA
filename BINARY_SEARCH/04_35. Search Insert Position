class Solution {
public:
    int searchInsert(vector<int>& nums, int target) {
        int n = nums.size();
        int s = 0, h = n-1;
        int idx = n;
        while(s <= h){
            int mid = s + (h-s)/2;
            if(nums[mid] >= target){
                idx = mid;
                h=mid-1;
            }else{
                s = mid+1;
            }
        }
        return idx;
    }
};
